<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script src="1.js"></script>
<script>
function zhugeshow__() {
	var y1 = document.body.scrollTop, y2 = y1 + document.body.clientHeight;
	var imgs = e_tn__("img");
	for(var i = 0; i < imgs.length; i++) {
		var img = imgs[i];
		var y = img.offsetTop;
		if(y + img.height >= y1 && y <= y2) {
			img.src = img.parentNode.parentNode.parentNode.title;
		}
	}
}
window.onscroll = zhugeshow__;
window.onload = zhugeshow__;

function for_tag__(f, all) {
	var tags = e_cn__("tag");
	for(var i = 0; i < tags.length; i++) {
		var tag = tags[i];
		if(tag.checked || all) {
			f(tag);
		}
	}
}
function tag__() {
	var s = "";
	for_tag__(function(tag) {
		if(s != "")
			s += " ";
		s += '"' + tag.nextSibling.title + '"';
	});
	return s;
}
function mv__() {
	var s = tag__();
	if(s == "")
		return;
	e__("tags").value = s;
	window.open("mv.zsp");
}
function rm__() {
	var s = tag__();
	if(s == "")
		return;
	if(!confirm("删除 " + s + " 吗？"))
		return
	exec__("rm -r " + s, function(ret) {
		if(ret != "") {
			alert("删除出错");
			return;
		}
		rm_sel__();
	});
}
function rm_sel__() {
	for_tag__(function(tag) {
		var n = tag.parentNode;
		n.parentNode.removeChild(n);
	});
	zhugeshow__();
}
function sel_all__(all) {
	for_tag__(function(tag) {
		tag.checked = all ? true : !tag.checked;
	}, true);
}
</script>
