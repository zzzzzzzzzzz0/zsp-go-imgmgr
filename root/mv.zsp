<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script src="1.js"></script>
<script>
var opener_;
function set_opener__(o) {
	opener_ = o;
	opener_.set_mv__(function(s) {
		focus__("");
		e__("tags").innerHTML = s;
	}, set_opener__);
}

function submit__(f) {
	var dir = f.dir.value;
	if(!dir) {
		var dirs = e_n__("dir");
		for(var i = 0; i < dirs.length; i++) {
			var d = dirs[i];
			if(d.checked) {
				dir = d.value;
				break;
			}
		}
	}
	if(dir) {
		var tags = e__("tags").innerHTML;
		if(tags != "") {
			exec__("mv -n " + tags + ' "' + dir + '"', function(ret) {
				if(ret != "") {
					alert("移动出错");
				} else {
					e__("tags").innerHTML = "";
					opener_.change__("mv");
					opener_.rm_sel__();
				}
			});
		}
	}
	return false;
}
set_opener__(opener);
window.onunload = function() {
	opener_.set_mv__(null, null);
};
</script>
<style>
#dirs {
	word-break:break-all;
	border:1px dashed #000;
	top:0;
	position:absolute;
	right:0;
	width:50%;
}
#tag {
	word-break:break-all;
	border:1px dashed #000;
	top:0;
	position:fixed;
	left:0;
	width:49%;
}
</style>
</head>
<body>
<form onsubmit="return submit__(this)">
<div id=tag>
<input type=submit value="移动"> 
<span id=tags></span> 
到：
</div>
