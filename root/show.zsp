#bar {
	position:fixed;
	z-index:1;
	top:0;
	padding:7;
	border:1px dashed #000;
	background-color:rgba(255, 255, 255, 0.65);
	text-align:center;
}
#bar2 {
	position:fixed;
	bottom:0;
	left:0;
	z-index:1;
	background-color:rgba(255, 255, 255, 0.65);
	font-size:11px;
}
.btn {padding:0 0 3 0;}
