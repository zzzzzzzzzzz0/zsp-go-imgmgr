<script>
var i_ = <%我的参%>;
function max__() {
	return opener.img_max_;
}

function w_h__(w, h) {
	img_.style.maxWidth = null;
	img_.style.width = w;
	img_.style.height = h;
}

function z__() {
	if(i_ < 1)
		i_ = 1;
	else if(i_ > max__())
		i_ = max__();
	var e = o_e__("i" + i_);
	if(!e) {
		old_key__();
		return;
	}
	src_ = e.value;
	img1_.src = src_;
	img_1_.style.marginTop = 0;
	r_ = 0; s_ = sx_ = sy_ = fx_ = 1;
	transform__();
	title__();
}
function z2__() {
	img_.src = img1_.src;
	w_h__(null, null);
	if(img1_.height > document.body.clientHeight) {
		img_.style.maxWidth = "auto";
		img_.style.height = "100%";
	}
	else if(img1_.width > document.body.clientWidth) {
		img_.style.maxWidth = "100%";
		img_.style.height = "auto";
	}
}
function title__() {
	e__("title").innerHTML =
		old_key_ + " " +
		i_ + ".." + max__() + " " +
		r_ + " " +
		//img1_.width + "x" + img1_.height + " " +
		//img_.width + "x" + img_.height + " " +
		src_;
}
var r_, s_, sx_, sy_, fx_;
function transform__() {
	img_.style.webkitTransform = img_.style.transform = 
		"rotate(" + r_ + "deg)scale(" + (fx_ * sx_) + "," + sy_ + ")";
}
/*function s__(s) {
	sx_ = sy_ = s; transform__();
}*/
function w_h_s__() {
	w_h__(img1_.width * s_, img1_.height * s_);
}
function s1__() {
	s_ = 1; w_h_s__();
}
function s__() {
	s_ += 0.01; w_h_s__();
}
function ss__() {
	s_ -= 0.01; w_h_s__();
}
function r__() {
	r_ += 15; transform__(); title__();
}
function rr__() {
	r_ -= 15; transform__(); title__();
}
function f__() {
	fx_ = fx_ == 1 ? -1 : 1; transform__();
}
function first__() {
	i_ = 1; z__();
}
function back__() {
	i_--; z__();
}
function next__() {
	i_++; z__();
}
function end__() {
	i_ = max__(); z__();
}
function rand__() {
	i_ = Math.floor(Math.random() * max__()) + 1; z__();
}
function old_key__() {
	switch(old_key_) {
	case "B":
		back__();
		break;
	default:
		next__();
		break;
	case "R":
		rand__();
		break;
	}
}
function set_change__(f) {
	opener.set_change__(function(s) {
		opener.set_change__(null);
		focus__(window, s);
		if(old_key_ == "B")
			back__();
		else 
			next__();
	});
	opener.sel_all__(false);
	o_e__("i" + i_).checked = true;
}
function mv__() {
	set_change__();
	opener.mv__();
}
function rm__() {
	set_change__();
	opener.rm__();
}
function quit__() {
	window.close();
}
function fs__() {
	if(document.fullScreen || document.mozFullScreen || document.webkitIsFullScreen) {
		var de = document;
		if (de.exitFullscreen) {
			de.exitFullscreen();
		} else if (de.mozCancelFullScreen) {
			de.mozCancelFullScreen();
		} else if (de.webkitCancelFullScreen) {
			de.webkitCancelFullScreen();
		}
	} else {
		var de = document.documentElement;
		if (de.requestFullscreen) {
			de.requestFullscreen();
		} else if (de.mozRequestFullScreen) {
			de.mozRequestFullScreen();
		} else if (de.webkitRequestFullScreen) {
			de.webkitRequestFullScreen();
		}
	}
}
function up__(up) {
	var s = img_1_.style, i = up ? -1 : 1;
	var i2 = parseInt(s.marginTop);
	if(!i2)
		i2 = 0;
	s.marginTop = i2 + i * 10;
}
function kao__() {
	var s = e__("bar").style;
	if(s.left == "0px") {
		s.right = "0px";
		s.left = null;
		e__("kao").innerHTML = "左";
	} else {
		s.left = "0px";
		s.right = null;
		e__("kao").innerHTML = "右";
	}
}
function bar_hide__() {
	var bar = e__("bar").style, bar2 = e__("bar2").style;
	if(bar.display != "none") {
		bar.display = "none";
		return;
	}
	if(bar2.display != "none") {
		bar2.display = "none";
		return;
	}
	bar.display = bar2.display = "block";
}
var old_key_ = "N";
window.document.onkeydown = function(evt) {
	var key = String.fromCharCode(evt.keyCode);
	switch(key) {
	case "B":
	case "N":
	//case "R":
	case "E":
		old_key_ = key;
		break;
	case " ":
		key = old_key_;
		break;
	}
	switch(key) {
	case "F":
		first__();
		break;
	case "B":
		back__();
		break;
	case "N":
		next__();
		break;
	case "E":
		end__();
		break;
	case "R":
		rand__();
		break;
	case "1":
		s1__();
		break;
	case "D":
		s__();
		break;
	case "S":
		ss__();
		break;
	case "X":
		r__();
		break;
	case "Z":
		rr__();
		break;
	case "Q":
		quit__();
		break;
	case "H":
		bar_hide__();
		break;
	case "O":
		up__(true);
		break;
	case "P":
		up__(false);
		break;
	}
};
</script>
<div id=bar style="left:0px">
